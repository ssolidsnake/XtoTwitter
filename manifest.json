{
  "manifest_version": 2,
  "name": "XtoTwitter",
  "description": "Simple extensi√≥n que hace el cambio del logo de X al antiguo logo de Twitter",
  "homepage_url": "https://ssolidsnake.github.io",
  "version": "1.0",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "64": "icons/icon64.png",
    "96": "icons/icon96.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*",
        "https://mobile.twitter.com/*"
      ],
      "js": [
        "content_script.js"
      ],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    //"script.js"
  ],
  "options_ui": {
    "browser_style": true,
    "chrome_style": false,
    "page": "options.html"
  },
  "browser_action": {
    "browser_style": true,
    "default_title": "XtoTwitter",
    "default_popup": "popup.html"
  },
  "permissions": [
  ]
}
